<body>
    <script>
        //          0  1  2  3  4
        let scores=[90,80,77,100,89];

        // 가장 큰 수를 저장할 변수
        // 초기값 설정은 배열의 첫 번째 값으로 초기화
        let max = scores[0];

        if( max < scores[1]){
            max = scores[1];
        }

        if( max < scores[2]){
            max = scores[2];
        }

        if (max <scores[3]){
            max=scores[3];
        }

        if ( max <scores[3]){
            max=scores[4];
        }

        console.log(max);
        
    </script>
</body>
